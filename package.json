{
  "name": "ahrq-dashboard",
  "version": "1.0.0",
  "private": true,
  "description": "AHRQ Research Impact Dashboard with Advanced Visualizations",
  "scripts": {
    "dev": "vite",
    "dev:full": "node start-dev-with-backend.js",
    "dev:backend": "cd backend && npm run dev",
    "dev:all": "./start-dev.sh",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "check-imports": "node scripts/check-imports.js",
    "prebuild": "npm run check-imports && npm run typecheck"
  },
  "dependencies": {
    "@types/d3": "^7.4.3",
    "@types/d3-sankey": "^0.12.4",
    "@types/papaparse": "^5.3.16",
    "d3": "^7.8.5",
    "d3-sankey": "^0.12.3",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.368.0",
    "papaparse": "^5.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.12.7",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@executeautomation/playwright-mcp-server": "^1.0.6",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
